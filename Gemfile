source 'https://rubygems.org'

gem 'rails'

# Gems used only for assets and not required
# in production environments by default.
group :development, :test do
  gem 'rspec-rails'
end

group :test do
	gem 'capybara'
  gem 'database_cleaner'
  gem 'factory_girl_rails'
end

group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'

  gem 'therubyracer'

  gem 'uglifier'
  gem 'twitter-bootstrap-rails'
end

gem 'jquery-rails'
gem 'mysql2'
gem 'devise' 
gem 'high_voltage'
gem 'carrierwave' 
gem 'rmagick'
gem 'thin'
gem 'will_paginate'

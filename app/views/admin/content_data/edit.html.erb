<div class="container eng" >
    <div class="edit_content">
        <%= form_for @data, :url => "/admin/content_data" do |f| %>
        <p> <%= t :owner_name %>:       <%= f.text_field :owner_name %></p>
        <p> <%= t :owner_second_name %>:<%= f.text_field :owner_second_name %></p>
        <p> <%= t :owner_last_name %>:	<%= f.text_field :owner_third_name %></p>
        <p> <%= t :address %>: 		<%= f.text_field :home_address %></p>
        <p> <%= t :trade_address %>:	<%= f.text_field :trade_address %></p>
        <p> <%= t :static_phone %>: 	<%= f.text_field :static_phone %></p>
        <p> <%= t :mobile_phone %>: 	<%= f.text_field :mobile_phone %></p>

        <%= f.submit "#{I18n.t :update}" %><%= f.submit "#{I18n.t :cancel}", :name => "cancel", :style=>"margin-left:95px"%>
        <% end %>
    </div>
</div>